<?xml version="1.0" encoding="UTF-8"?>
<project name="rake-targets" default="echoProperties">

    <import file="./artd-rakish/bin/artd-rakish-bin/call-rake.xml" />

    <property environment="env" />

    <target name="echoProperties" description="dump properties to console">
        <echoproperties/>
    </target>

    <target name="autogen" description="generate code">
        <call-rake file="./rakefile.rb" target="autogen" />
    </target>

    <target name="nativeLibs" description="Build native libraries">
        <call-rake file="./rakefile.rb" target="nativeLibs" />
    </target>


</project>